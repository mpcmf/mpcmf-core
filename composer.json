{
  "name": "mpcmf/mpcmf-core",
  "version": "1.0.0-dev",
  "description": "Build fast and powerful console and web applications. Part of mpcmf solution.",
  "keywords": [
    "cache",
    "threads",
    "react",
    "mongodb",
    "rabbitmq",
    "console",
    "applications",
    "slim",
    "smarty",
    "mvc"
  ],
  "type": "library",
  "repositories": [
      {"type": "vcs", "url": "https://github.com/pltrm/elasticsearch"}
  ],
  "require": {
    "php": ">=5.5.10",
    "ext-json":  "*",
    "ext-intl":  "*",
    "ext-posix":  "*",
    "ext-memcached":  "*",
    "ext-amqp":  "*",
    "ext-sockets":  "*",
    "ext-zlib":  "*",
    "ext-pcntl":  "*",
    "ext-mbstring":  "*",
    "slim/slim": "2.*",
    "symfony/console": "~2.6",
    "phpdocumentor/reflection-docblock": "~2.0",
    "symfony/filesystem": "~2.6",
    "phpmailer/phpmailer": "~6.0",
    "monolog/monolog": "~1.12",
    "bramus/monolog-colored-line-formatter": "2.x",
    "khr/react-curl": "dev-master",
    "infostars/net-curl": "dev-master",
    "smarty/smarty": "~3.1",
    "alcaeus/mongo-php-adapter": "^1.1@dev",
    "nervetattoo/elasticsearch": "dev-master",
    "mpcmf/mpcmf-signal-handler": "dev-master",
    "morris/lessql": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Gregory Ostrovsky",
      "email": "greevex@gmail.com",
      "homepage": "https://github.com/greevex"
    },
    {
      "name": "Dmitry Emelyanov",
      "email": "gilberg.vrn@gmail.com",
      "homepage": "https://github.com/gilberg-vrn"
    }
  ],
  "support": {
    "issues": "https://github.com/greevex/mpcmf-core/issues?state=open",
    "source": "https://github.com/greevex/mpcmf-core",
    "email": "greevex@gmail.com"
  },
  "autoload": {
    "psr-4": {
      "mpcmf\\system\\": "src/system/"
    },
    "files": [
      "src/loader.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "prefer-lowest": false
}
